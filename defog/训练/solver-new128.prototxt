net: "/home/zehao/shi-yan-2/train-new128.prototxt"
test_iter: 50
test_interval: 500
# lr for fine-tuning should be lower than when starting from scratch
base_lr: 0.001
lr_policy: "step"
gamma: 0.5
# stepsize should also be lower, as we're closer to being done
stepsize: 50000
display: 500
max_iter: 500000
momentum: 0.9
weight_decay: 0.0005
snapshot: 10000
snapshot_prefix: "/home/zehao/shi-yan-2/models/model-new128/"
# uncomment the following to default to CPU mode solving
 solver_mode: GPU
